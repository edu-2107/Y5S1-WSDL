@prefix onto: <http://example.org/ontomaint#> .
@prefix sosa: <http://www.w3.org/ns/sosa/> .
@prefix s4inma: <https://saref.etsi.org/saref4inma/> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .

onto:Machine           a owl:Class ;
  rdfs:subClassOf s4inma:ProductionEquipment .

onto:Job               a owl:Class .
onto:ErrorContext      a owl:Class .
onto:FailurePropagation a owl:Class .
onto:CorrectiveAction  a owl:Class .
onto:MaintenanceTask   a owl:Class .
onto:Component         a owl:Class .
onto:Operator          a owl:Class .
onto:Team              a owl:Class .
onto:SparePart         a owl:Class .
onto:ProductionBatch   a owl:Class .
onto:PerformanceMetric a owl:Class .

# associates error to the machine
onto:affectsMachine    a owl:ObjectProperty ;
  rdfs:domain onto:ErrorContext ;
  rdfs:range  onto:Machine .

# associates error to the task that stopped
onto:blocksJob         a owl:ObjectProperty ;
  rdfs:domain onto:ErrorContext ;
  rdfs:range  onto:Job .

# associates error to its solution
onto:requiresAction    a owl:ObjectProperty ;
  rdfs:domain onto:ErrorContext ;
  rdfs:range  onto:CorrectiveAction .

# which jobs fail next due to the 1st one
onto:nextJob           a owl:ObjectProperty ;
  rdfs:domain onto:Job ;
  rdfs:range  onto:Job .

# points to next error
onto:propagatesTo      a owl:ObjectProperty ;
  rdfs:domain onto:FailurePropagation ;
  rdfs:range  onto:ErrorContext .

# points to original error
onto:hasCause          a owl:ObjectProperty ;
  rdfs:domain onto:FailurePropagation ;
  rdfs:range  onto:ErrorContext .

# associates machine to its sensor
onto:monitoredBy       a owl:ObjectProperty ;
  rdfs:domain onto:Machine ;
  rdfs:range  sosa:Sensor .

# integer number to represent severity
onto:hasSeverity a owl:DatatypeProperty ;
  rdfs:domain onto:ErrorContext ;
  rdfs:range xsd:integer .

# cost of error in time
onto:hasDowntimeMinutes a owl:DatatypeProperty ;
  rdfs:domain onto:ErrorContext ;
  rdfs:range xsd:integer .

# Machine and component properties
onto:hasComponent a owl:ObjectProperty ;
  rdfs:domain onto:Machine ;
  rdfs:range onto:Component .

onto:hasCapacity a owl:DatatypeProperty ;
  rdfs:domain onto:Machine ;
  rdfs:range xsd:float .

onto:hasAgeYears a owl:DatatypeProperty ;
  rdfs:domain onto:Machine ;
  rdfs:range xsd:float .

onto:manufacturingYear a owl:DatatypeProperty ;
  rdfs:domain onto:Machine ;
  rdfs:range xsd:gYear .

onto:lastMaintenanceDate a owl:DatatypeProperty ;
  rdfs:domain onto:Machine ;
  rdfs:range xsd:dateTime .

onto:maintenanceIntervalDays a owl:DatatypeProperty ;
  rdfs:domain onto:Machine ;
  rdfs:range xsd:integer .

# Operator and team properties
onto:operatedBy a owl:ObjectProperty ;
  rdfs:domain onto:Machine ;
  rdfs:range onto:Operator .

onto:maintainedBy a owl:ObjectProperty ;
  rdfs:domain onto:Machine ;
  rdfs:range onto:Team .

onto:belongsToTeam a owl:ObjectProperty ;
  rdfs:domain onto:Operator ;
  rdfs:range onto:Team .

onto:hasExperienceLevel a owl:DatatypeProperty ;
  rdfs:domain onto:Operator ;
  rdfs:range xsd:integer .

onto:hasSpecialty a owl:DatatypeProperty ;
  rdfs:domain onto:Operator ;
  rdfs:range xsd:string .

# Maintenance task properties
onto:requiresMaintenance a owl:ObjectProperty ;
  rdfs:domain onto:Machine ;
  rdfs:range onto:MaintenanceTask .

onto:isScheduledFor a owl:ObjectProperty ;
  rdfs:domain onto:MaintenanceTask ;
  rdfs:range xsd:dateTime .

onto:dueDate a owl:DatatypeProperty ;
  rdfs:domain onto:MaintenanceTask ;
  rdfs:range xsd:dateTime .

onto:maintenanceDescription a owl:DatatypeProperty ;
  rdfs:domain onto:MaintenanceTask ;
  rdfs:range xsd:string .

onto:estimatedDurationHours a owl:DatatypeProperty ;
  rdfs:domain onto:MaintenanceTask ;
  rdfs:range xsd:float .

# Spare parts
onto:requiresSparePart a owl:ObjectProperty ;
  rdfs:domain onto:CorrectiveAction ;
  rdfs:range onto:SparePart .

onto:partNumber a owl:DatatypeProperty ;
  rdfs:domain onto:SparePart ;
  rdfs:range xsd:string .

onto:leadTimeDays a owl:DatatypeProperty ;
  rdfs:domain onto:SparePart ;
  rdfs:range xsd:integer .

onto:costUSD a owl:DatatypeProperty ;
  rdfs:domain onto:SparePart ;
  rdfs:range xsd:float .

# Production batch and metrics
onto:processesBatch a owl:ObjectProperty ;
  rdfs:domain onto:Machine ;
  rdfs:range onto:ProductionBatch .

onto:batchSize a owl:DatatypeProperty ;
  rdfs:domain onto:ProductionBatch ;
  rdfs:range xsd:integer .

onto:batchDate a owl:DatatypeProperty ;
  rdfs:domain onto:ProductionBatch ;
  rdfs:range xsd:dateTime .

# Performance and monitoring
onto:hasPerformanceMetric a owl:ObjectProperty ;
  rdfs:domain onto:Machine ;
  rdfs:range onto:PerformanceMetric .

onto:metricName a owl:DatatypeProperty ;
  rdfs:domain onto:PerformanceMetric ;
  rdfs:range xsd:string .

onto:metricValue a owl:DatatypeProperty ;
  rdfs:domain onto:PerformanceMetric ;
  rdfs:range xsd:float .

onto:metricUnit a owl:DatatypeProperty ;
  rdfs:domain onto:PerformanceMetric ;
  rdfs:range xsd:string .

onto:measurementTime a owl:DatatypeProperty ;
  rdfs:domain onto:PerformanceMetric ;
  rdfs:range xsd:dateTime .

onto:failureRate a owl:DatatypeProperty ;
  rdfs:domain onto:Machine ;
  rdfs:range xsd:float .

onto:uptime a owl:DatatypeProperty ;
  rdfs:domain onto:Machine ;
  rdfs:range xsd:float .
